{
  "name": "speer-assessment",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "db:dev:rm": "docker compose rm assessment-db -s -f -v",
    "db:dev:up": "docker compose up assessment-db -d",
    "prebuild": "rimraf dist",
    "db:dev:restart": "yarn db:dev:rm && yarn db:dev:up && sleep 15 && yarn migration:run",
    "prestart:dev": "yarn db:dev:restart",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest-unit.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "stop:test:e2e": "./graceful-shutdown.sh",
    "start:test:e2e": "dotenv -e .env.test -- node dist/src/main 1>test/logs/test.log &",
    "test:e2e": "cucumber-js",
    "pretest:e2e": "yarn stop:test:e2e && yarn db:test:restart && yarn start:test:e2e && sleep 15",
    "db:test:rm": "dotenv -e .env.test -- docker compose rm test-assessment-db -s -f -v && docker volume prune -f --filter all=1",
    "db:test:up": "dotenv -e .env.test -- docker compose up test-assessment-db -d",
    "db:test:migrate": "dotenv -e .env.test -- yarn migration:run",
    "db:test:fixtures:load": "dotenv -e .env.test -- fixtures-ts-node-commonjs load ./fixtures --dataSource=./dist/db/data-source.js",
    "db:test:restart": "yarn db:test:rm && yarn db:test:up && sleep 15 && yarn db:test:migrate && yarn db:test:fixtures:load",
    "typeorm": "yarn build && npx typeorm -d dist/db/data-source.js",
    "migration:generate": "yarn typeorm migration:generate",
    "migration:run": "yarn typeorm migration:run"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/typeorm": "^10.0.2",
    "argon2": "^0.30.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-typeorm": "^2.0.0",
    "cookie-parser": "^1.4.6",
    "cron-parser": "^4.9.0",
    "cron-validator": "^1.3.1",
    "docxtemplater": "^3.37.14",
    "express-session": "^1.17.3",
    "mysql2": "^2.3.3",
    "nestjs-pino": "^3.1.2",
    "pino-http": "^8.3.3",
    "pino-mongodb": "^4.3.0",
    "pizzip": "^3.1.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.10",
    "typeorm-transactional": "^0.4.1"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.226.0",
    "@compodoc/compodoc": "^1.1.21",
    "@cucumber/cucumber": "^8.10.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cookie-parser": "^1.4.5",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.9",
    "@types/jest": "28.1.8",
    "@types/luxon": "^3.1.0",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "adr": "^1.4.3",
    "docsify-cli": "^4.4.4",
    "docsify-mermaid": "^2.0.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "is-ci": "^3.0.1",
    "jest": "28.1.3",
    "lint-staged": "^13.1.0",
    "pactum": "^3.3.1",
    "pactum-matchers": "^1.1.4",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typeorm-fixtures-cli": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  }
}
